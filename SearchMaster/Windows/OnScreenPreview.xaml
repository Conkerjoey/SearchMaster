<Window x:Class="SearchMaster.Windows.OnScreenPreview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchMaster.Windows"
        mc:Ignorable="d"
        Title="OnScreenPreview" Height="450" Width="800"
        WindowStyle="None"
        ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="listBoxElement" d:ItemsSource="{d:Binding Elements}" />
    </Grid>
</Window>
